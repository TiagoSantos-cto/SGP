@model EntregaModel

@{
    var vCodigoEntrega = string.Empty;

    try
    {
        vCodigoEntrega = @ViewBag.Registro.Id.ToString();
    }
    catch { }
}

<div class="alert-success container body-content text-center" style="padding-top: 100px; bottom:auto; border-radius: 10px; text-align: center; padding: 20px; width: 500px; font-family: Calibri ">
    <h3><strong>Operação realizada com sucesso. Número: @vCodigoEntrega </strong> <br /> <a href="..\..\Home\Index"> sair </a> </h3>
</div>